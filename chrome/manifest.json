{
  "name": "TSUNTSUN",
  "version": "1.0",
  "manifest_version": 3,
  "description": "TSUNTSUN 積みます",
  "homepage_url": "https://tsuntsun.herokuapp.com/",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://*/", "http://*/"],
      "run_at": "document_start",
      "js": ["jquery-3.6.0.min.js", "index.js"]
    }
  ],
  "action": {
    "default_title": "TSUNTSUN",
    "default_popup": "index.html",
    "default_icon": "tsunmae.png"
  }
}
